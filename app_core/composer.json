{
	"name": "earosb/iipzs",
	"description": "Sistema Icil Icafal Proyecto Zona Sur S.A., icilicafalpzs.cl",
	"type": "project",
    "authors": [
      {
        "name": "Eduardo Aros",
        "email": "earosb@gmail.com"
      }
    ],
	"require": {
		"laravel/framework": "4.2.*",
		"mrjuliuss/syntara": "1.2.*",
		"nesbot/Carbon": "*",
		"fitztrev/laravel-html-minify": "1.*",
		"maatwebsite/excel": "1.*"
	},
	"require-dev": {
		"way/generators": "~2.0",
		"barryvdh/laravel-debugbar": "~1.8"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
            "app/library/Components"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
